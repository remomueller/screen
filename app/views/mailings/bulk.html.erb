<% @title = 'Mailings Import' %>
<div class="page-header">
  <h1>
    <%= @title %>
    <small>Spreadsheet - Tab Delimited</small>
  </h1>
</div>

<%= form_tag import_mailings_path, method: :post, class: 'form-horizontal' do %>
  <table style="width:100%;table-layout:fixed">
    <thead>
      <tr>
        <th>Cardiologist</th>
        <th>Date of Mailing</th>
        <th>MRN</th>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Address1</th>
        <th>City</th>
        <th>State</th>
        <th>Zip Code</th>
        <th>Home Phone</th>
        <th>Day Phone</th>
      </tr>
    </thead>
  </table>
  <%= text_area_tag :tab_dump, '', rows: 7, style: "width:99%" %>
  <div class="form-actions">
    <%= submit_tag "Import", class: 'btn btn-primary' %>
  </div>
<% end %>
