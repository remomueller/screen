<% @title = 'Clinics' %>
<div class="page-header">
  <h1>
    <%= @title %>
    <%#= link_to "Create Clinic", new_clinic_path, class: 'btn btn-primary' %>
  </h1>
</div>

<%= form_tag clinics_path, method: :get, class: 'form-inline' do %>
  <%= hidden_field_tag :order, params[:order] %>
  <%= text_field_tag 'search', params[:search], class: 'form-control' %>
  <%= submit_tag 'Search', class: 'btn btn-primary', name: nil %>
  <%= link_to 'Reset', clinics_path, class: 'btn btn-default' %>
<% end %>

<%= render partial: 'clinics/clinics' %>
